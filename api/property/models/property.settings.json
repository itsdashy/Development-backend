{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "name": "Property"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "price": {
      "type": "decimal"
    },
    "development": {
      "model": "developments",
      "via": "properties"
    }
  }
}
